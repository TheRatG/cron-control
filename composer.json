{
    "name": "therat/cron-control",
    "description": "Manage all your cron jobs without modifying crontab",
    "type": "project",
    "license": "MIT",
    "keywords": [
        "Symfony3",
        "crontab"
    ],
    "homepage": "http://theratg.github.io/cron-control/",
    "authors": [
        {
            "name": "Vladimir Pak",
            "email": "TheRatW@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "TheRat\\": "src/TheRat/"
        },
        "classmap": [
            "app/AppKernel.php"
        ]
    },
    "require": {
        "php": ">=5.5.9",
        "doctrine/collections": "^1.3",
        "herrera-io/phar-update": "^2.0",
        "mtdowling/cron-expression": "^1.1.0",
        "swiftmailer/swiftmailer": "^5.4",
        "symfony/console": "^2.8|^3.0",
        "symfony/dependency-injection": "^2.8|^3.0",
        "symfony/filesystem": "^2.8|^3.0",
        "symfony/finder": "^2.8|^3.0",
        "symfony/monolog-bridge": "^2.8|^3.0",
        "symfony/options-resolver": "^2.8|^3.0",
        "symfony/process": "^2.8|^3.0",
        "symfony/yaml": "^2.8|^3.0",
        "symfony/config": "^2.8|^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "symfony/var-dumper": "^2.8|^3.0",
        "phpro/grumphp": "^0.9.0",
        "squizlabs/php_codesniffer": "^2.7"
    },
    "support": {
        "email": "support@exemple.org",
        "issues": "https://github.com/TheRat/cron-control/issues"
    },
    "bin": [
        "bin/cron-control"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "minimum-stability": "stable"
}
